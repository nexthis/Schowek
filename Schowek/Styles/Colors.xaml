<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Schowek.Style">


    <Color x:Key="BackgroundLight">#67cbf7</Color>
    <SolidColorBrush x:Key="BackgroundLightBrush" Color="{StaticResource BackgroundLight}" />

    <Color x:Key="BackgroundSetting">#aae5ff</Color>
    <SolidColorBrush x:Key="BackgroundSettingBrush" Color="{StaticResource BackgroundSetting}" />

    <Color x:Key="BackgroundMain">#16BCE0</Color>
    <SolidColorBrush x:Key="BackgroundMainBrush" Color="{StaticResource BackgroundMain}" />

    <Color x:Key="BorderFileControl">#FFCFCF7F</Color>
    <SolidColorBrush x:Key="BorderFileControlBrush" Color="{StaticResource BorderFileControl}" />

    <Color x:Key="BorderImageControl">#FFC87FCF</Color>
    <SolidColorBrush x:Key="BorderImageControlBrush" Color="{StaticResource BorderImageControl}" />

    <Color x:Key="BorderMusicControl">#FF7FC0CF</Color>
    <SolidColorBrush x:Key="BorderMusicControlBrush" Color="{StaticResource BorderMusicControl}" />

    <Color x:Key="BorderTextControl">#89cf7f</Color>
    <SolidColorBrush x:Key="BorderTextControlBrush" Color="{StaticResource BorderTextControl}" />

</ResourceDictionary>